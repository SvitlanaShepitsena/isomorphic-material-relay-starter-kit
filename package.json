{
  "name": "isomorphic-material-relay-starter-kit",
  "description": "Starter kit for isomorphic SPA using React/Relay, Material-UI, JWT authentication, Apache Cassandra and samples",
  "author": "Aleksandar Chalakov, Code Foundries and contributors",
  "license": "MIT",
  "homepage": "https://github.com/codefoundries/isomorphic-material-relay-starter-kit",
  "version": "0.7.7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SvitlanaShepitsena/isomorphic-material-relay-starter-kit.git"
  },

  "keywords": [
    "SPA",
    "single page application",
    "isomorphic",
    "progressive enhancement",
    "server rendering",
    "authentication",
    "JWT",
    "Material Design",
    "material-ui",
    "react",
    "relay",
    "isomorphic-relay",
    "isomorphic-relay-router",
    "graphql",
    "react-hot-loader",
    "express",
    "webpack",
    "babel",
    "babel 6",
    "Cassandra",
    "Cassandra DB",
    "cassandra-driver"
  ],
  "engines": {
    "node": "4.2.4",
    "npm": "3.3.6"
  },
  "scripts": {
    "postinstall": "npm run build-schema&&npm run build-webpack",
    "setup-local": "cp ./doc/Example.env ./.env",
    "init": "cqlsh -f ./scripts/cassandra-init.cql",
    "build-schema": "./node_modules/.bin/babel-node ./scripts/build-schema.js",
    "build-webpack": "./node_modules/.bin/webpack -p --output-filename app.js",
    "build-mui-icon-list": "./node_modules/.bin/babel-node ./scripts/build-mui-icon-list.js",
    "config-da-memory": "./node_modules/.bin/replace da_cassandra da_memory ./data/da/*",
    "config-da-cassandra": "./node_modules/.bin/replace da_memory da_cassandra ./data/da/*",
    "start-webpack": "better-npm-run start-webpack",
    "start-dev": "better-npm-run start-dev",
    "start": "set NODE_ENV=production&&babel-node ./server/server.js",
    "dev": "concurrent --kill-others \"npm run start-webpack\" \"npm run start-dev\""
  },
  "betterScripts": {
    "start-dev": {
      "command": "nodemon --exec babel-node -- ./server/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-webpack": {
      "command": "webpack-dev-server --hot --inline --progress --colors"
    }
  },
  "metadata": {
    "graphql": {
      "schema": "./graphql/schema.json"
    }
  },
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-plugin-transform-async-to-generator": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-relay-plugin": "^0.6.3",
    "babel-relay-plugin-loader": "^0.6.3",
    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.1",
    "cassandra-driver": "^3.0.0",
    "chalk": "^1.1.1",
    "classnames": "^2.2.0",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.19.0",
    "dotenv": "^1.2.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "express-graphql": "^0.4.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "graphql": "^0.4.12",
    "graphql-relay": "^0.3.4",
    "isomorphic-relay": "^0.4.2",
    "isomorphic-relay-router": "^0.5.1",
    "jwt-simple": "^0.4.0",
    "material-ui": "^0.14.3",
    "nib": "^1.1.0",
    "node-fetch": "^1.3.3",
    "nodemon": "^1.7.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-helmet": "^2.3.1",
    "react-hot-loader": "^1.3.0",
    "react-relay": "^0.6.1",
    "react-router": "^2.0.0-rc4",
    "react-tap-event-plugin": "^0.2.1",
    "replace": "^0.3.0",
    "seq-queue": "0.0.5",
    "squel": "^4.2.2",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.3.13",
    "recursive-readdir-sync": "^1.0.6",
    "webpack-dev-server": "^1.14.0",
    "better-npm-run": "^0.0.5",
    "concurrently": "^0.1.1"
  }
}
